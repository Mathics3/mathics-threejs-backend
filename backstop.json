{
	"id": "backstop_default",
	"paths": {
		"bitmaps_reference": "backstop_data/bitmaps_reference",
		"bitmaps_test": "backstop_data/bitmaps_test",
		"html_report": "backstop_data/html_report"
	},
	"report": [
		"browser"
	],
	"engine": "puppeteer",
	"engineOptions": {
		"args": [
			"--no-sandbox",
			"--disable-setuid-sandbox"
		]
	},
	"asyncCaptureLimit": 5,
	"asyncCompareLimit": 50,
	"debug": false,
	"debugWindow": false,
	"viewports": [
		{
			"label": "main",
			"width": 400,
			"height": 400
		}
	],
	"scenarios": [
		{
			"label": "scene with axes",
			"url": "http://localhost:8080/examples/test/axes.html",
			"delay": 1000,
			"misMatchThreshold": 0.6
		},
		{
			"label": "wrong protocol version",
			"url": "http://localhost:8080/examples/test/wrong_protocol_version.html",
			"delay": 1000,
			"requireSameDimensions": false,
			"misMatchThreshold": 7
		},
		{
			"label": "Ambient light",
			"url": "http://localhost:8080/examples/test/ambient_light.html",
			"delay": 1000,
